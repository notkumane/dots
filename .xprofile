#!/bin/sh
xset s off &
xset -dpms &
xset s noblank &
xset r rate 300 50 &
xrdb ~/.Xresources &
xrandr --output HDMI-0 --primary --mode 1920x1080 --pos 0x0 --rotate normal --output VGA-0 --mode 1600x900 --pos 1920x0 --rotate normal &
xcompmgr -f &
/usr/lib/xfce4/notifyd/xfce4-notifyd &
pa-applet &
conky &
unclutter &
wal -R &
